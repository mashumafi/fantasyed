#scoreboard-standing(data-role="page")
	div(data-id="header", data-role="header", data-position="fixed")
		a(href="#home", data-icon="home") Home
		h1 Fantasy Ed
		a(href="#feed-chat", data-icon="bars") Mail
		div(data-role="navbar")
			ul
				li
					a(href="#scoreboard") Team
				li
					a(href="#scoreboard-league") League
				li
					a.ui-btn-active.ui-state-persist(href="#scoreboard-standing") Standings
	div(data-role="content")
		div(data-role="collapsible", data-collapsed="false")
			h2 Wild Life
			p
				table.ui-responsive.table-stroke(data-role="table", data-mode="reflow")
					thead
						tr
							th(data-priority=1)  
							th(data-priority=2) Team
							th(data-priority=3) Wins
							th(data-priority=4) Loses
							th(data-priority=5) Score
					tbody
						tr
							td
								img(src="img/team/wild_life/eagle.png", height="80")
							td Eagles
							td 3
							td 2
							td 83
						tr
							td
								img(src="img/team/wild_life/koala.png", height="80")
							td Koala
							td 4
							td 1
							td 71
						tr
							td
								img(src="img/team/wild_life/panda.png", height="80")
							td Panda
							td 2
							td 3
							td 68
		div(data-role="collapsible")
			h2 League name
			p
	div(data-id="footer", data-role="footer", data-position="fixed")
		div(data-role="navbar")
			ul
				li
					a.ui-btn-active.ui-state-persist(href="#scoreboard", data-icon="grid") Scoreboard
				li
					a(href="#team", data-icon="grid") Team
				li
					a(href="#feed", data-icon="grid") Feed
				li
					a(href="#leaderboard", data-icon="grid") Leaderboard